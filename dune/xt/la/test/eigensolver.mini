__name = la_eigensolver
__exec_suffix = {matrix_short}

matrix = EigenDenseMatrix<double>, FieldMatrix<double\, 2\, 2> | expand types
matrix_short = eigendense, fieldmatrix | expand types
complex_matrix = EigenDenseMatrix<std::complex<double>>, FieldMatrix<std::complex<double>\, 2\, 2> | expand types

EIGEN3_FOUND, 1 | expand types | cmake_guard

[__static]
TESTMATRIXTYPE = {matrix}
COMPLEXTESTMATRIXTYPE = {complex_matrix}
